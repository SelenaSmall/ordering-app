{% trans_default_domain 'app' %}
<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}{{ 'app.title'|trans }}{% endblock %}</title>
{% stylesheets 'assets/css/index.css' %}
            <link rel="stylesheet" media="all" href="{{ asset_url }}" />
{% endstylesheets %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body>
       <header role="banner" class="navbar navbar-default">
           <div class="container-fluid">
               <a href="{{ url('OrderBundle:form') }}" class="navbar-brand">{{ 'app.title'|trans }}</a>

               <nav role="navigation">
                    <ul class="nav navbar-nav">
                        <li><a href="{{ url('OrderBundle:form') }}">{{ 'view.form.title'|trans({}, 'OrderBundle') }}</a></li>
                        <li><a href="{{ url('OrderBundle:list') }}">{{ 'view.list.title'|trans({}, 'OrderBundle') }}</a></li>
                    </ul>
               </nav>
           </div>
       </header>

       <main role="main" class="container">
           <div class="page-header">
{% block header %}{% endblock %}
           </div>
{% block content %}{% endblock %}
       </main>

{% block javascripts %}{% endblock %}
    </body>
</html>
